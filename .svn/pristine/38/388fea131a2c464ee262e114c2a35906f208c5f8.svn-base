package com.geecity.hisenseplus.home.bean;

/**
 *  新房列表类
 * 
 * @author Administrator
 * 
 */
public class EstateListNewBean {
	private int id;
	private String lpmc;
	private String cityName;
	private String quyu;
	private String zt;
	private String xzst;
	private String jiageLeixing;
	private long jiage;

	public String getDescription(){
		return getCityName() + "  " + getQuyu();
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getLpmc() {
		return lpmc;
	}

	public void setLpmc(String lpmc) {
		this.lpmc = lpmc;
	}

	public String getCityName() {
		return cityName;
	}

	public void setCityName(String cityName) {
		this.cityName = cityName;
	}

	public String getQuyu() {
		return quyu;
	}

	public void setQuyu(String quyu) {
		this.quyu = quyu;
	}

	public String getZt() {
		return zt;
	}

	public void setZt(String zt) {
		this.zt = zt;
	}

	public String getXzst() {
		return xzst;
	}

	public void setXzst(String xzst) {
		this.xzst = xzst;
	}

	public String getJiageLeixing() {
		return jiageLeixing;
	}

	public void setJiageLeixing(String jiageLeixing) {
		this.jiageLeixing = jiageLeixing;
	}

	public long getJiage() {
		return jiage;
	}

	public void setJiage(long jiage) {
		this.jiage = jiage;
	}

	@Override
	public String toString() {
		return "EstateListNewBean [id=" + id + ", lpmc=" + lpmc + ", cityName="
				+ cityName + ", quyu=" + quyu + ", zt=" + zt + ", xzst=" + xzst
				+ ", jiageLeixing=" + jiageLeixing + ", jiage=" + jiage + "]";
	}
}
