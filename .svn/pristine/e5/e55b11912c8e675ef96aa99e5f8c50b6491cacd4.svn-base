package com.geecity.hisenseplus.home.bean;

import java.io.Serializable;

/**
 * 商户详情的商户信息类
 * 
 * @author Administrator
 * 
 */
public class BusinessDetailBean implements Serializable {

	/**
     * 
     */
	private static final long serialVersionUID = 1L;
	
	private int store_id;
	private String store_logo;// 名称
	private String store_banner;//顶部图片
	private String store_name;
	private String store_ad;// 商家详情中间部分的图片地址
	private String tel;
	private int visits;

	public String getStore_ad() {
		return store_ad;
	}

	public void setStore_ad(String store_ad) {
		this.store_ad = store_ad;
	}

	public String getStore_banner() {
        return store_banner;
    }

    public void setStore_banner(String store_banner) {
        this.store_banner = store_banner;
    }

    public int getStore_id() {
		return store_id;
	}

	public void setStore_id(int store_id) {
		this.store_id = store_id;
	}

	public String getStore_logo() {
		return store_logo;
	}

	public void setStore_logo(String store_logo) {
		this.store_logo = store_logo;
	}

	public String getStore_name() {
		return store_name;
	}

	public void setStore_name(String store_name) {
		this.store_name = store_name;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public int getVisits() {
		return visits;
	}

	public void setVisits(int visits) {
		this.visits = visits;
	}

	@Override
	public String toString() {
		return "BusinessDetailBean [store_id=" + store_id + ", store_logo="
				+ store_logo + ", store_name=" + store_name + ", tel=" + tel
				+ ", visits=" + visits + "]";
	}

}
