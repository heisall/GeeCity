<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/layout_bg_gray" >

    <include
        android:id="@+id/layout_top"
        layout="@layout/layout_top_bar" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/layout_top"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="手机号码"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/text_size_18" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="5dp"
        android:background="@color/line_light_gray" />

    <EditText
        android:id="@+id/et_address_add_phone"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/textView12"
        android:background="@color/txt_white"
        android:ems="11"
        android:hint="请输入手机号码"
        android:inputType="phone"
        android:paddingLeft="15dp"
        android:textColorHint="@color/txt_hint_gray"
        android:textSize="@dimen/text_size_16" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/et_address_add_phone"
        android:background="@color/line_light_gray" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView13"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="联系人姓名"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/text_size_18" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="5dp"
        android:background="@color/line_light_gray" />

    <EditText
        android:id="@+id/et_address_add_name"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/textView22"
        android:background="@color/txt_white"
        android:hint="请输入姓名"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textColorHint="@color/txt_hint_gray"
        android:textSize="@dimen/text_size_16" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/et_address_add_name"
        android:background="@color/line_light_gray" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView23"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:visibility="gone"
        android:text="所在城市"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/text_size_18" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:visibility="gone"
        android:layout_below="@+id/textView31"
        android:layout_marginTop="5dp"
        android:background="@color/line_light_gray" />

    <EditText
        android:id="@+id/et_address_add_city"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="50dp"
        android:layout_below="@+id/textView32"
        android:background="@color/txt_white"
        android:hint="请输入城市(如：青岛)"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textColorHint="@color/txt_hint_gray"
        android:textSize="@dimen/text_size_16" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/et_address_add_city"
        android:background="@color/line_light_gray" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView33"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="详细地址"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/text_size_18" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView41"
        android:layout_marginTop="5dp"
        android:background="@color/line_light_gray" />

    <EditText
        android:id="@+id/et_address_add_detail"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/textView42"
        android:background="@color/txt_white"
        android:hint="请输入详细地址"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textColorHint="@color/txt_hint_gray"
        android:textSize="@dimen/text_size_16" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/et_address_add_detail"
        android:background="@color/line_light_gray" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView43"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:text="设为默认地址"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/text_size_18" />

    <CheckBox
        android:id="@+id/cb_address_add_isdefault"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignBaseline="@+id/textView51"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@+id/textView51"
        android:button="@drawable/btn_house_bind_agree_selector" />

    <Button
        android:id="@+id/btn_address_add_save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_margin="20dp"
        android:background="@drawable/btn_send"
        android:text="完成"
        android:textColor="@drawable/text_white_gray" />

</RelativeLayout>