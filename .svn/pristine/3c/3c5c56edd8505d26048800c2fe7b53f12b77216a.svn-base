<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_person"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/layout_bg_gray"
    android:orientation="vertical" >

    <include
        android:id="@+id/person_layout"
        layout="@layout/layout_top_bar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/layout_bg_gray" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_head"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_account"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <com.geecity.hisenseplus.home.view.RoundImageView
                    android:id="@+id/img_pers_head"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_alignTop="@+id/imageView1"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView1"/>

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_nickname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_tel"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_pers_nickname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView4"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView4"
                    android:layout_toRightOf="@+id/tv_pers_02"
                    android:ellipsize="end"
                    android:gravity="right|center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_gender"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_pers_edit_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView5"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView5"
                    android:layout_toRightOf="@+id/tv_pers_05"
                    android:ellipsize="end"
                    android:gravity="right|center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_city"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_pers_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView6"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView6"
                    android:layout_toRightOf="@+id/tv_pers_03"
                    android:ellipsize="end"
                    android:gravity="right|center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_like"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_pwd"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_pers_like"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView7"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView7"
                    android:layout_toRightOf="@+id/tv_pers_04"
                    android:ellipsize="end"
                    android:gravity="right|center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="15dp"
                android:background="@color/line_light_gray" />

            <RelativeLayout
                android:id="@+id/ly_pers_sign"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_person_comm_selector"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_pers_06"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/pers_actv_edit_item_ins"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_pers_sign"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView8"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView8"
                    android:layout_toRightOf="@+id/tv_pers_06"
                    android:ellipsize="end"
                    android:gravity="right|center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/txt_black"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/arrow_right" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_light_gray" />

            
        </LinearLayout>
    </ScrollView>

</LinearLayout>