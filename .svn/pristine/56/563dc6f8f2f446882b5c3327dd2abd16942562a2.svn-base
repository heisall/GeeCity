package com.geecity.hisenseplus.home.bean;

import java.io.Serializable;

public class NoticeDetailBean implements Serializable {

	/**
     * 
     */
	private static final long serialVersionUID = 1L;
	
	private int id;// ID
	private int A_ID;
	private String Title;// 标题
	private String Description;
	private String Content;// 详情页链接地址
	private int isurl;//
	private String Types;//
	private int IsAllowEnroll;// 是否允许报名
	private int IsEnrollEnd;// 报名是否截至(只有允许报名，并且没有截至的活动，可以报名)
	private String EnrollBegionTime;
	private String EnrollEndTime;
	private String EnrolCount;
	private String ClickCount;
	private String AddPerson;
	private String AddTime;
	private int canEnroll;

	public int getID() {
		return id;
	}

	public void setID(int iD) {
		id = iD;
	}

	public int getA_ID() {
		return A_ID;
	}

	public void setA_ID(int a_ID) {
		A_ID = a_ID;
	}

	public String getTitle() {
		return Title;
	}

	public void setTitle(String title) {
		Title = title;
	}

	public String getDescription() {
		return Description;
	}

	public void setDescription(String description) {
		Description = description;
	}

	public String getContent() {
		return Content;
	}

	public void setContent(String content) {
		Content = content;
	}

	public int getIsurl() {
		return isurl;
	}

	public void setIsurl(int isurl) {
		this.isurl = isurl;
	}

	/**
	 * 通知类型
	 * 
	 * @return 1-通知 2-活动
	 */
	public String getTypes() {
		return Types;
	}

	public void setTypes(String types) {
		Types = types;
	}

	public int getIsAllowEnroll() {
		return IsAllowEnroll;
	}

	public void setIsAllowEnroll(int isAllowEnroll) {
		IsAllowEnroll = isAllowEnroll;
	}

	public int getIsEnrollEnd() {
		return IsEnrollEnd;
	}

	public void setIsEnrollEnd(int isEnrollEnd) {
		IsEnrollEnd = isEnrollEnd;
	}

	public String getEnrollBegionTime() {
		return EnrollBegionTime;
	}

	public void setEnrollBegionTime(String enrollBegionTime) {
		EnrollBegionTime = enrollBegionTime;
	}

	public String getEnrollEndTime() {
		return EnrollEndTime;
	}

	public void setEnrollEndTime(String enrollEndTime) {
		EnrollEndTime = enrollEndTime;
	}

	public String getEnrolCount() {
		return EnrolCount;
	}

	public void setEnrolCount(String enrolCount) {
		EnrolCount = enrolCount;
	}

	public String getClickCount() {
		return ClickCount;
	}

	public void setClickCount(String clickCount) {
		ClickCount = clickCount;
	}

	public String getAddPerson() {
		return AddPerson;
	}

	public void setAddPerson(String addPerson) {
		AddPerson = addPerson;
	}

	public String getAddTime() {
		return AddTime;
	}

	public void setAddTime(String addTime) {
		AddTime = addTime;
	}

	public int getCanEnroll() {
		return canEnroll;
	}

	public void setCanEnroll(int canEnroll) {
		this.canEnroll = canEnroll;
	}
}
